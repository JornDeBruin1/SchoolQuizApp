{
	"hosting": {
		"source": "dist",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"frameworksBackend": {
			"region": "europe-west1"
		},
		"headers": [
			{
				"source": "./public/manifest.json",
				"headers": [
					{
						"key": "Content Type",
						"value": "application/json"
					}
				]
			}
		]
	}
}
